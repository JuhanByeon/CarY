<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 타입 별칭 지정  -->
	<typeAliases>
		<typeAlias type="com.project.cary.domain.CarInfoVO" alias="CarInfo"/>
		<typeAlias type="com.project.cary.domain.CategoryVO" alias="Category"/>
		<typeAlias type="com.project.cary.domain.UserVO" alias="User"/>
		<typeAlias type="com.project.cary.domain.CsVO" alias="Cs"/>
		<!--
		<typeAlias type="com.project.cary.domain.PostVO" alias="Post"/>
		-->
	</typeAliases>
	
	<!-- mapper파일 등록 -->
	<mappers>
		<mapper resource="spring/mapper/CarMapper.xml"/>
		<mapper resource="spring/mapper/CategoryMapper.xml"/>
		<mapper resource="spring/mapper/UserMapper.xml"/>
		<mapper resource="spring/mapper/CSMapper.xml"/>
		<!--
		<mapper resource="spring/mapper/PostMapper.xml"/>
		-->
	</mappers>
</configuration>


